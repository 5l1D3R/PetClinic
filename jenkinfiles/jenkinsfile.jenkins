pipeline{
    agent any
    
   tools{
        maven 'Maven 3.8.4'
    }
    
    stages{
        stage('Build&Test'){
            steps {
                bat 'mvn clean package'
            }
        }
        
    }
	post{
        always{
            emailtext body: "${currentBuild.currentResult}: J"
        }   
    }
    
}
