pipeline {
    agent any
    stages {
        stage('get') {
            steps {
                git branch: 'relese1.0', url: 'https://github.com/akhilbabu459/spring-petclinic.gitgit '
            }
        }
        stage('build') {
            steps {
                sh 'mvn package'
            }
        }
        stage('Archive JUnit formatted test results') {
            steps {
                junit '**/surefire-reports/*.xml'
            }
        }
        stage('Archive artifacts') {
            steps {
            archive '**/target/*.jar'
            }
        }
    }    
}