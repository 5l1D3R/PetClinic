node ('jdk11-mvn3.8.5'){
    stage ('git'){
        credentialsId: 'JENKINS_BUILD_USER',
        url:'https://github.com/SriSuryaTej/spring-petclinic.git'
    }
    stage ('build'){
        sh 'mvn clean package'
    }
    stage ('execute'){
        sh 'java -jar ~/jenkins/workspace/target/*.jar &'
    }
}