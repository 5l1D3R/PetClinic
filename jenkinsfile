pipeline {
    agent {label 'java11'}
    stages {
        stage('vcs') {
          steps {
            git branch: 'main', url :'https://github.com/spring-projects/spring-petclinic.git'
          }  
        }
        stage ('build') {
            steps {
                sh '/usr/share/maven/bin/mvn package'
            }
        }
        stage ('archieve results') {
            steps 'junit **/surefire-reports/*.xml'
        }
    }
}