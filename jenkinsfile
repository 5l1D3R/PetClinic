pipeline {
    agent {'openjdk-11-maven'}
    stages {
        stage (vcs){
            git branch: 'new',
             url:'https://github.com/neelamkannan/spring-petclinic.git'
            }
            stage (build) {
                sh '/usr/share/maven/bin/mvn package'
            }
            stage ('archive results') {
            steps {
                junit '**/surefire-reports/*.xml'
        }
    }