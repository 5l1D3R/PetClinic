pipeline {
    agent { label 'maven' }
    stages {
        stage('vcs') {
            steps {
                git url: 'https://github.com/naresh010203/spring-petclinic.git',
                branch: 'develop'
            }
        }
        stages('build') {
            steps {
                sh 'export PATH="/usr/lib/jvm/java-1.17.0-openjdk-amd64/bin:$PATH"'
                sh 'export PATH="/opt/maven/bin:$PATH"'
                sh 'mvn package'
            }
        }
    }
}