pipeline {
    agent any
    parameters { choice(name: 'MYBRANCH', choices: ['main', 'master'], description: 'maintain multiple branches'), 
               string(name: 'MVNPACK', defaultValue: 'package', description: 'USER MAVEN') }
    stages {
        stage('clone') {
            steps {
             git url: 'https://github.com/nagarjunaduggireddy/spring-petclinicmain.git', branch: '$MYBRANCH'
            }
        }
         stage('Deploy') {
            steps {
                sh 'mvn $"MVNPACK"'
            }
        }
        stage('JTest') {
            steps {
                junit testResults: '**/surefire-reports/*.xml'
            }
        }

    }
}
