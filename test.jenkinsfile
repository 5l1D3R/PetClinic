pipeline {
    agent any
    stages {
        stage('clone') {
            steps {
             git url: 'https://github.com/nagarjunaduggireddy/spring-petclinicmain.git'
            }
        }
        stage('JTest') {
            steps {
                junit testResults: '**/surefire-reports/*.xml'
            }
        }
        stage('Deploy') {
            steps {
                sh 'mvn package'
            }
        }
    }
}
